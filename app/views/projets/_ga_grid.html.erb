<%= grid(@autres_grid) do |g|
	g.column in_csv: false do |m|
		link_to('<i class="glyphicon glyphicon-pencil"></i>'.html_safe,  [:edit, @projet,m])
	end  
	g.column in_csv: false do |m|
		link_to '<i class="glyphicon glyphicon-trash"></i>'.html_safe, [m.projet, m], method: :delete, data: { confirm: 'Are you sure?' }
	end
