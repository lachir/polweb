	end
	g.column  name: 'Unité PCI' do |task| 
		task.unite_pci
	end
	g.column  name: 'Teneur en soufre' do |task| 
		task.teneur_soufre_defaut
	end
	g.column  name: 'Teneur en chlore' do |task| 
		task.teneur_chlore_defaut
	end
	g.column  name: 'Teneur en Azote' do |task| 
		task.teneur_azote_defaut
	end
	g.column  name: 'Teneur en carbone' do |task| 
		task.teneur_carbone_defaut
	end
	g.column  name: 'Teneur en hydrogène' do |task| 
		task.teneur_hydrogene_defaut
	end
	g.column  name: 'teneur en Na2O' do |task| 
		task.teneur_na2o_defaut						
	end
	g.column  name: 'teneur en K2O' do |task| 
		task.teneur_k2o_defaut						
	end
